{
  "manifest_version": 2,
  "name": "seb-spoof-ext",
  "version": "1.0.1",
  "description": "Spoofs the usage of Safe Exam Browser",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "activeTab",
    "<all_urls>"
  ]
}
